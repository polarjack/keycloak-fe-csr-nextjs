# Keycloak Configuration
# Copy this file to .env.local and update with your Keycloak server details

# The URL of your Keycloak server
NEXT_PUBLIC_KEYCLOAK_URL=http://localhost:8080

# The Keycloak realm name
NEXT_PUBLIC_KEYCLOAK_REALM=master

# The Keycloak client ID for this application
NEXT_PUBLIC_KEYCLOAK_CLIENT_ID=mpc-mvp-client
